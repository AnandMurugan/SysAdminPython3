#!/usr/bin/env python3.7

user = {'admin':False, 'active':False, 'name':'Kevin'}

if user['active']:
    if user['admin']:
        print('ACTIVE - (ADMIN) %s '% (user['name']))
    else:
        print('ACTIVE - %s' % (user['name']))
elif user['admin']:
    print('(ADMIN) %s' % (user['name']))
else:
    print('%s' % (user['name']))

